<h1>Messages</h1>

<div class="contents">
  <div class="js-message-errors"></div> 

  <%= form_with(model: @message, class: 'js-form') do |f| %>
    <%= f.text_field :body %>
    <%= f.submit %>
  <% end %>
  <ul class="messages">
    <%= render @messages %> <%# _message.html.erbを呼び出す %>
  </ul>
</div>